{
  "name": "move-hackathon-server",
  "version": "1.0.0",
  "description": "Express server for Move Hackathon",
  "main": "dist/server.js",
  "scripts": {
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 tsx watch src/index.ts",
    "db:migrate": "tsx --env-file=.env src/db/migrate.ts",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "build": "tsc",
    "start": "tsc && node dist/index.js",
    "db:studio": ""
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": "^1.37.1",
    "@langchain/anthropic": "^0.3.15",
    "@langchain/core": "^0.3.43",
    "@langchain/langgraph": "^0.2.57",
    "@langchain/openai": "^0.4.9",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.41.0",
    "express": "^4.21.2",
    "move-agent-kit": "^0.2.0",
    "openai": "^4.89.0",
    "pg": "^8.11.3",
    "postgres": "^3.4.5",
    "tsx": "^4.19.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^22.13.13",
    "@types/pg": "^8.11.11",
    "drizzle-kit": "^0.30.5",
    "typescript": "^5.8.2"
  }
}